{
  "name": "tokens-to",
  "version": "0.0.1",
  "description": "Converter for css and js tokens",
  "main": "./build/index.js",
  "scripts": {
    "test": "jest",
    "transpile": "npx tsc",
    "rm-buildinfo": "rm tsconfig.tsbuildinfo",
    "build": "npm run rm-buildinfo & npm run transpile",
    "tokens-to": "node ./bin/tokens-to-cli.js",
    "js2css": "node ./bin/tokens-to-cli.js js2css",
    "js2json": "node ./bin/tokens-to-cli.js js2json"
  },
  "bin": {
    "tokens-to": "./bin/tokens-to.js"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./bin/tokens-to": "./bin/tokens-to.js",
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "keywords": [
    "js",
    "ts",
    "css",
    "json",
    "token",
    "style",
    "object",
    "convert",
    "transpile"
  ],
  "author": "HoJoon Eum",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "js-beautify": "^1.14.11"
  },
  "peerDependencies": {
    "glob": "^10.3.10",
    "js-beautify": "^1.14.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^16.10.0"
  }
}
